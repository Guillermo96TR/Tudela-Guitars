<!DOCTYPE html>

<title>Guitars</title>

{% block body %}
    <h1>Guitars</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ guitar.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ guitar.nombre }}</td>
            </tr>
            <tr>
                <th>Caracteristicas</th>
                <td>{{ guitar.caracteristicas }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ guitar.price }}</td>
            </tr>
            <tr>
                <th>Imagen</th>
                <td>{{ guitar.imagen }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_guitars_index') }}">back to list</a>

    <a href="{{ path('app_guitars_edit', {'id': guitar.id}) }}">edit</a>

    {{ include('guitars/_delete_form.html.twig') }}
